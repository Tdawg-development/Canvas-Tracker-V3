{
  "name": "canvas-tracker-v3",
  "version": "3.0.0",
  "description": "Canvas Tracker v3: Rebuilt with rigorous architecture and strict development practices",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "test": "cross-env NODE_ENV=test VERBOSE_TESTS=true jest",
    "test:watch": "cross-env NODE_ENV=test VERBOSE_TESTS=true jest --watch",
    "test:coverage": "cross-env NODE_ENV=test VERBOSE_TESTS=true jest --coverage",
    "test:canvas": "node test-canvas-staging.js",
    "test:canvas-staging": "jest canvas-interface/tests --verbose",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:migrate": "knex migrate:latest --knexfile src/infrastructure/persistence/knex/knexfile.ts",
    "db:migrate:rollback": "knex migrate:rollback --knexfile src/infrastructure/persistence/knex/knexfile.ts",
    "db:seed": "knex seed:run --knexfile src/infrastructure/persistence/knex/knexfile.ts",
    "clean": "rimraf dist",
    "commit": "powershell -ExecutionPolicy Bypass -File scripts/commit.ps1",
    "demo": "powershell -ExecutionPolicy Bypass -File scripts/run-demo.ps1",
    "demo:list": "powershell -ExecutionPolicy Bypass -File scripts/run-demo.ps1 -List",
    "demo:interactive": "powershell -ExecutionPolicy Bypass -File scripts/run-demo.ps1 -Interactive"
  },
  "keywords": [
    "canvas",
    "education",
    "student-tracking",
    "clean-architecture",
    "typescript"
  ],
  "author": "Tdawg-development",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "knex": "^3.1.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "pg": "^8.12.0",
    "redis": "^4.7.0",
    "uuid": "^10.0.0",
    "winston": "^3.14.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.7",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "^2.6.11",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.12.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
